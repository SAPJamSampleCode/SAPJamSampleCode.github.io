<?xml version="1.0" ?>
<control xmlns="http://www.sap.com/sap.ui.library.xsd" >

	<name>HexagonButtonGroup</name>

	<documentation>A group of HexagonButtons, aligned in a packed grid</documentation>

	<baseType>sap.ui.core/Control</baseType>
	<properties>
		<property group="Misc" name="colspan" type="sap.ui.core/int" defaultValue="3">
			<documentation>How many buttons might be placed in the same row of the grid</documentation>
		</property>
	</properties>
	<aggregations>
		<aggregation cardinality="0..n" name="buttons" type="sap.ui.demokit/HexagonButton">
			<documentation>The buttons to layout in a grid</documentation>
		</aggregation>
	</aggregations>
</control>
