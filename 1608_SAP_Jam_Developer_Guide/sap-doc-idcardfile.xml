<?xml version="1.0" encoding="UTF-8"?>
<build>
  <output>
    <rootpage>index.html</rootpage>
    <sap.dita.src.pattern>^(.*)\.([^.]*)$</sap.dita.src.pattern>
    <sap.dita.loio.pattern>^loio(.*)$</sap.dita.loio.pattern>
    <sap.dita.dest.pattern>\1.html</sap.dita.dest.pattern>
    <sap.dita.O2O.pattern>$1.html</sap.dita.O2O.pattern>
    <sap.dita.O2O.t2t.pattern />
  </output>
</build>
